    "def experiment():\n",
    "    bandits = [Bandit(p) for p in bandit_prob] # assign initial probs to bandits\n",
    "    rewards = np.zeros(n_trials) # Keep track of rewards\n",
    "    c = 2 # EXPLORATION COEFFICIENT, controls degree of exploration, must be greater than 0\n",
    "    t = 0 # timestep/trial index\n",
    "    n_optimal = 0\n",
    "    optimal_j = np.argmax([b.p for b in bandits]) # find the optimal bandit\n",
    "    print(\"Optimal Bandit: \",optimal_j)\n",
    "\n",
    "    for b in bandits:\n",
    "        #Pull each arm once and then update - the initial exploration phase\n",
    "        x = b.pull() # pull arm\n",
    "        rewards[t] = x  # update rewards\n",
    "        b.update(x) # update estimated probability for bandit\n",
    "        t+=1\n",
    "\n",
    "    for i in range(t, n_trials):\n",
    "        # Exploitation phase\n",
    "        \n",
    "        # get the bandit with the highest confidence\n",
    "        # We follow the formula for this       \n",
    "        # c*np.sqrt(np.log(i)/b.N) the upper confidence bound which is added to the probability estimate\n",
    "        \n",
    "        j = np.argmax([b.p_estimate + c*np.sqrt(np.log(i)/b.N) for b in bandits])\n",
    "\n",
